{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mo",
      "camelcase": "Mo",
      "title": "Mo",
      "scope": "source.mo",
      "file-types": [
        "mo"
      ],
      "injection-regex": "^mo$",
      "class-name": "TreeSitterMo"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter parser for the Mo programming language",
    "authors": [
      {
        "name": "earomc"
      }
    ],
    "links": {
      "repository": "https://github.com/molanguage/tree-sitter-mo"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}